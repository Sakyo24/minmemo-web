{
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "arrowParens": "always",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": ["resources/views/**/*.blade.php"],
      "options": {
        "tabWidth": 4,
        "parser": "blade"
      }
    }
  ]
}
